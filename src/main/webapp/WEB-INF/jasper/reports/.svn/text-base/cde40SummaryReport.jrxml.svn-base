	<queryString>
		<![CDATA[select  m.organization_code as DISTRICT_NUMBER,
      m.organization_name,
      t.OPER_TRAN,
      t.MIG_MILES,
      t.REG_D_MILES,
      t.SCHOOL_DAYS,
      t.BOARD_TRAN,
      t.PUPIL_TRAN,
      t.ACT_MILES,
      t.TOTAL_MILES,
      t.CAP_OUT_TRAN,
      t.REIM_TRAN_LY,
      t.ADV_PAY
from  organization_unit_master m
left outer join transportation t
on  t.district_number = m.organization_code
and   t.FISCAL_YEAR =$P{fiscalYear}
where  m.ORGANIZATION_UNIT_TYPE in ('DISTRICT','CSI')
 and   substr(m.organization_code, 0,1) != 't'
 and m.DATE_OPERATIONAL <= sysdate
 and m.DATE_CLOSED IS NULL
 or  m.organization_code in ('9025','9701')
order by m.organization_code]]>
	</queryString>
