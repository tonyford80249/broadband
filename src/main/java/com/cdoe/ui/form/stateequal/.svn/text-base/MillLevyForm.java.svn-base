    public Double getSumLines1Thru4() {
        final double ZERO = 0.0;
        
        double totalProgramMillLevy = this.calcTotalProgram != null ? this.calcTotalProgram : ZERO;
        double categoricalBuyoutMillLevu = this.calcCategoricalBuyout != null ? this.calcCategoricalBuyout : ZERO;
        double overrideMillLevy = this.calcVoterApproved != null ? this.calcVoterApproved : ZERO;
        double holdHarmlessMillLevy = this.calcHoldHarmless != null ? this.calcHoldHarmless : ZERO;
        double excessHoldHarmlessMillLevy = this.calcExcessHold != null ? this.calcExcessHold : ZERO;
        
        return totalProgramMillLevy + categoricalBuyoutMillLevu + overrideMillLevy + holdHarmlessMillLevy + excessHoldHarmlessMillLevy;
    }

